<h1>Kladr#index</h1>
<p>Find me in app/views/kladr/index.html.erb</p>

<% unless @regions.blank? %>
  <% @regions.each do |region| %>
    <p>
      <%= region.code[0..1] %>
      <%= link_to region.name, kladr_show_path(:code => region.code) %>
      <%= region.code %>
    </p>
  <% end %>
<% end %>